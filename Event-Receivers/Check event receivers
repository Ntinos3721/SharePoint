# https://naimmurati.wordpress.com/2012/03/22/add-modify-or-delete-list-event-receivers-with-powershell/Add-PSSnapin Microsoft.SharePoint.PowerShell –erroraction SilentlyContinue $web = Get-SPWeb -Identity https://dms/apps/pnpdemands/ #http://dmsdev2013:81$list = $web.GetList($web.Url + "/Lists/timologia") $type = "ItemDeleting" #or any other type, like ItemDeleting, ItemAdding, ItemUpdating, ... $numberOfEventReceivers = $list.EventReceivers.CountWrite-Host "EventReceivers count: " $numberOfEventReceivers if ($numberOfEventReceivers -gt 0){   for( $index = $numberOfEventReceivers -1; $index -gt -1; $index–-)   {      $receiver = $list.EventReceivers[$index] ;      $name = $receiver.Name      $typ = $receiver.Type ;       Write-Host "Event receiver" $name      if ($typ -eq $type)  #or you can check ($name -eq "event receiver's name") if you have more then one event receivers of the same type      {         Write-Host "Event receiver" $name "with type" $type      }   }}else{   Write-Host " There is no EventReceivers of type " $type " registered for this list "} $web.Dispose()
